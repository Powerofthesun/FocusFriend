{
  "manifest_version": 2,

  "name": "Focus Friend",
  "description": "This extension prevents you from being distracted while at your computer",
  "version": "1.0",

"background": {
  "scripts": ["background.js"]
    },
    "icons": { "16": "icon.png",
           "48": "48.png",
          "128": "128.png" },
"browser_action": {
          "default_icon": "icon.png",
          "default_title": "Focus Friend",
          "default_popup": "popup.html" 
        },
  "options_page": "options.html",
  "permissions": [
      "tabs",
    "activeTab",
    "storage",
    "alarms"
  ]
}